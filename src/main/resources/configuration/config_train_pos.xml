<configuration>
    <language>english</language>

	<tsv>
        <column index= "0" field="form"/>
        <column index= "1" field="pos"/>
        <column index="-1" field="feats"/>
    </tsv>

    <trainer algorithm="adagrad" label_cutoff="4" feature_cutoff="3" reset="false" average="false" learning_rate="0.01" decaying_rate="0.4" batch_ratio="0.1" bias="0" />
	<ambiguity_class_threshold>0.4</ambiguity_class_threshold>
	<bootstrap delta="0.01"/>
</configuration>
